          rdfs:range [ rdf:type owl:Class ;
                       owl:unionOf ( :CompuestoQuimico
                                     :Enfermedad
                                     :Medicamento
                                     :Sintoma
                                   )
                     ] ;
          rdfs:label "menciona"@es .


